
{
  "name": "shayannosrat/laravel-blog-ai",
  "description": "Blog Article Generation with GPT for laravel 8.0 and up",
  "keywords": [
    "laravel",
    "blog",
    "blogarticle",
    "gpt",
    "ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/shayannosrat/laravel-blog-ai",
  "authors": [
    {
      "name": "Shayan Nosratzadegan",
      "email": "nosratzadegan@datlogic.de",
      "homepage": "https://shayannosrat.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/database": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^9.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Shayannosrat\\BlogAi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shayannosrat\\BlogAi\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev",
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Shayannosrat\\BlogAi\\BlogAiServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "php-cs-fixer fix --allow-risky=yes",
    "analyse": "phpstan analyse"
  }
}
